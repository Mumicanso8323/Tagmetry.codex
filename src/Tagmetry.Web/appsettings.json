{
  "Paths": {
    "LogsDir": "log",
    "ThirdPartyDir": "third_party"
  },
  "JoyTag": {
    "Enabled": true,
    "UseGpu": true,
    "Host": "127.0.0.1",
    "Port": 7865,
    "PythonExeRelativePath": "joytag/runtime/.venv/Scripts/python.exe",
    "ServerScriptRelativePath": "joytag/runtime/joytag_server/server.py"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Warning",
      "Microsoft.AspNetCore.Routing.EndpointMiddleware": "Warning",
      "Microsoft.AspNetCore.Http.Result": "Warning"
    }
  }
}
